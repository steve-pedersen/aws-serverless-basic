{
  "name": "aws-serverless-basic",
  "version": "1.0.0",
  "description": "AWS Serverless Basic Starter App",
  "scripts": {
    "lint": "eslint .",
    "deploy": "yarn install-layer && serverless deploy",
    "build": "yarn install-layer && serverless package",
    "install-layer": "yarn install --production --modules-folder dependency_layer/nodejs/node_modules",
    "test": "jest --coverage",
    "typescript": "typescript",
    "decrypt": "serverless decrypt",
    "encrypt": "serverless encrypt"
  },
  "jest": {
    "moduleNameMapper": {
      "^~(.*)$": "<rootDir>/src$1"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steve-pedersen/aws-serverless-basic"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.10.1",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.2.1",
    "prettier": "^2.0.5",
    "serverless": "^3.22.0",
    "serverless-offline": "^10.0.2",
    "serverless-secrets-plugin": "^0.1.0",
    "serverless-webpack": "^5.3.2",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
