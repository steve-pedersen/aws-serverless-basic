{
  "name": "aws-serverless-basic",
  "version": "1.0.0",
  "description": "AWS Serverless Basic Starter App",
  "scripts": {
    "lint": "eslint .",
    "deploy": "yarn install-layer && serverless deploy",
    "build": "yarn install-layer && serverless package",
    "install-layer": "yarn install --production --modules-folder dependency_layer/nodejs/node_modules",
    "test": "jest --coverage",
    "typescript": "typescript",
    "decrypt": "serverless decrypt",
    "encrypt": "serverless encrypt"
  },
  "jest": {
    "moduleNameMapper": {
      "^~(.*)$": "<rootDir>/src$1"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steve-pedersen/aws-serverless-basic"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.10.1",
    "@types/node": "^18.11.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "eslint": "^8.2.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^29.2.1",
    "prettier": "^2.0.5",
    "serverless": "^3.22.0",
    "serverless-offline": "^10.0.2",
    "serverless-secrets-plugin": "^0.1.0",
    "serverless-webpack": "^5.3.2",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "5.40.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {}
}
